<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.measurement.server.session">

  <class name="MonitorableType" table="EAM_MONITORABLE_TYPE">
    <cache usage="read-write"/>

    <id name="id" type="integer">
      <column name="ID" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">EAM_MONITORABLE_TYPE_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property name="name" type="string" unique="true">
      <column name="NAME" not-null="true" length="100" index="TYPE_NAME_IDX"/>
    </property>

    <property name="cid" type="integer">
      <column name="CID"/>
    </property>

    <property name="appdefType" type="integer">
      <column name="APPDEF_TYPE" not-null="true" index="MTYPE_IDX_APPDEFTYPE_IDX"/>
    </property>

    <property name="plugin" type="string">
      <column name="PLUGIN" not-null="true" length="250"/>
    </property>
  </class>

</hibernate-mapping>