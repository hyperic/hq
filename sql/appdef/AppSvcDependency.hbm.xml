<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.appdef">

  <class name="AppSvcDependency" table="EAM_SERVICE_DEP_MAP">
    <id name="id" type="integer">
      <meta attribute="scope-set"><![CDATA[private]]></meta>
      <column name="ID" not-null="true"/>
      <generator class="org.hyperic.hibernate.id.ComboGenerator">
        <param name="sequence">EAM_SERVICE_DEP_MAP_ID_SEQ</param>
      </generator>
    </id>

    <property name="creationTime" type="long">
      <column name="CTIME" not-null="false"/>
    </property>

    <property name="modifiedTime" type="long">
      <column name="MTIME" not-null="false"/>
    </property>

    <properties name="appSvcDepId" unique="true">
      <many-to-one name="appService" class="AppService">
        <column name="APPSERVICE_ID" not-null="false"/>
      </many-to-one>

      <many-to-one name="dependentService" class="AppService">
        <column name="DEPENDENT_SERVICE_ID" not-null="false"/>
      </many-to-one>
    </properties>
  </class>

</hibernate-mapping>
