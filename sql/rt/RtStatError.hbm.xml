<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.rt">

  <class name="RtStatError" table="EAM_STAT_ERRORS">
    <id name="id" type="integer">
      <column name="ID" not-null="true"/>
      <generator class="org.hyperic.hibernate.id.ComboGenerator">
        <param name="sequence">EAM_STAT_ERRORS_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property name="errorCount" type="integer">
      <column name="COUNT" not-null="true"/>
    </property>

    <many-to-one name="rtErrorCode" class="RtErrorCode">
      <column name="ERROR_ID"/>
    </many-to-one>

    <many-to-one name="rtRequestStat" class="RtRequestStat">
      <column name="REQSTAT_ID" index="STAT_ERRORS_REQSTAT"/>
    </many-to-one>
  </class>

</hibernate-mapping>
