          <metric name="Memory Usage Byte Count" units="b"/>
          <metric name="Memory Usage Portion" units="percent"/>
          <metric name="Cursor Memory Usage" units="b"/>
          <metric name="Cursor Percent Usage" units="percent"/>
      </service>

