    <service name="Topic">
      <property name="OBJECT_NAME" value="${TOPIC_OBJECT_NAME}"/>

      <config include="destination"/>

      <plugin type="autoinventory"/>

      <plugin type="control"
              class="org.hyperic.hq.product.jmx.MxControlPlugin"/>

